<?xml version="1.0" encoding="UTF-8"?>

<!-- NOTE: JOIN is child of COLLECTION.                                           -->

<dm-mapping:VODML
	xmlns:dm-mapping="http://www.ivoa.net/xml/merged-syntax">

	<dm-mapping:MODEL name="model" />
	<dm-mapping:MODEL name="model" url="http://aaaaaa" />
	<dm-mapping:GLOBALS>
		<dm-mapping:COLLECTION dmrole="abc">
			<!-- Test Case 9.4: must not have both dmref and tableref -->
			<dm-mapping:JOIN tableref="aaaaa" dmref="dddd" />
		</dm-mapping:COLLECTION>

	</dm-mapping:GLOBALS>
</dm-mapping:VODML>
